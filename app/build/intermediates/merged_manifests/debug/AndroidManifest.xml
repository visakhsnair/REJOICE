    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="32" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/apprejoice"
        android:label="@string/app_name"
        android:roundIcon="@drawable/apprejoice"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Rejoice" >
        <activity
            android:name="com.example.rejoice.personalitydevelopment"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.dancemusic"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.sports"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.OtherActivity"
