        <activity
            android:name="com.example.rejoice.Schooling"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.Rejoicepage"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.Registerpage"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.Loginpage"
            android:exported="false" />
        <activity
            android:name="com.example.rejoice.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.rejoice.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>